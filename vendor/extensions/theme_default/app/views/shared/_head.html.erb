<title><%= title %></title>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
<link rel="shortcut icon" href="/images/favicon.png" />
<%= stylesheet_tags %>
<%= javascript_include_tag 'jquery-1.3.2.min', 'jquery.validate.min.js', :cache => 'jquery-core' %>
<% unless I18n.locale.to_s[0..1] == 'en' -%>
  <%= javascript_include_tag ['localization/messages_', I18n.locale.to_s[0..1], '.js'].join %>
<% end -%>
<link href='http://fonts.googleapis.com/css?family=IM+Fell+Great+Primer&subset=latin' rel='stylesheet' type='text/css'>
<%= meta_data_tags %> 
<%= yield :head %>
